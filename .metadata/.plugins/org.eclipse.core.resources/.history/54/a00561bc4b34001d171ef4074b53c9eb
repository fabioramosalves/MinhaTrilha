package classe;

public class ValorVsReferencia {

	public static void main(String[] args) {

		double a = 2;
		double b = a;
		a++;
		b--;
		System.out.println(a);
		System.out.println(b);
		
		Data d1 = new Data(1,6,2022);
		
		var d2 = d1;
		
		d1.dia= 31;		
		d2.dia= 12;
		
		System.out.printf("%d %d", d1 , d2 );
	}
}
