package estruturaDeControle;

public class DesafioFor {
	public static void main(String[] args) {

		String valor = "#";
		for(valor.equals("#"); !valor.equals("#######"); valor.concat("#")) {
			System.out.println(valor);
			valor += "#";
		}
		
		//Versão do desafio
		// não pode usar o valor numerico o laço
	}
}
