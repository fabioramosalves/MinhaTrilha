package classe;

public class Jantar {
	
	public static void main(String[] args) {
	
		Pessoa p1 = new Pessoa("Fabio", 80);
		
		var c1 = new Comida("Feijoada", 0.5d);
		var c2 = new Comida("Carne", 0.2d);
		var c3 = new Comida("Arroz", 0.3d);
		
		p1.comer(c1);
		p1.comer(c2);
		p1.comer(c3);
		
		System.out.printf("%s %s ", p1.nome, p1.peso );
	}
}
