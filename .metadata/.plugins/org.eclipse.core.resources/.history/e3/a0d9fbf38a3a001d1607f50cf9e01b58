package lambdas;

public class Threads {

	public static void main(String[] args) {

		Runnable trabalho1 = new Trabalho1();

		Runnable trabalho2 = new Runnable() {

			public void run() {
				for (int i = 0; i < 100000000; i++) {
					System.out.println("Tarefa #02 " + i++);
				}
			}
		};

		trabalho1.run();
		trabalho2.run();

	}
}
