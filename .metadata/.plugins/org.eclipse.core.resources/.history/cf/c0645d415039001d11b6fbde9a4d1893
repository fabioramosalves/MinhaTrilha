package oo.heranca;

public class Jogo {

	public static void main(String[] args) {

		Jogador j1 = new Jogador();
		j1.x = 10;
		j1.y = 10;
		
		Jogador j2 = new Jogador();
		j2.x = 10;
		j2.y = 11;
		
		
		
		System.out.printf("Jogador 1 x -> %d, y -> %d " , j1.x, j1.y);
		System.out.printf("Jogador 2 x -> %d, y -> %d " , j2.x, j2.y);
		
		j1.atacar(j2);
	}
}
