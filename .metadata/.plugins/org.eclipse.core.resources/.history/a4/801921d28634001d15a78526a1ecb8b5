package arrays;

import java.util.Iterator;

public class Foreach {

	public static void main(String[] args) {
		
		double[] a = {1,2,3,4,5};
		
	    for (int i = 0; i < a.length; i++) {
	    	System.out.println(a[i]);
		}
		
	    System.out.println("--------------");
	    
		for (double d : a) {
			System.out.println(d);
		}
	}
}
