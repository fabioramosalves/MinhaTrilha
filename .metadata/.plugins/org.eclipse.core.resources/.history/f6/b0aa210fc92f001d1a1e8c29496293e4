package fundamentos;

public class DesafioOperadoresAritmeticos {

	public static void main(String[] args) {
		
		double a = 6 * (3 + 2);
		double b = Math.pow(a, 2);
		double c = b / 3 * 2;
		double e = (1 - 5 ) * ( 2 - 7 )/2;
		double f = Math.pow(e, 2);
		double g = Math.pow(c - f,3);
		double h = g / Math.pow(10, 3);	
		
	}
}
